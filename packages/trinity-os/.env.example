# TRINITY-OS Environment Variables
# 이 파일을 .env로 복사하여 설정하세요

# Python 환경
PYTHONPATH=/Users/brnestrm/AFO/TRINITY-OS
TRINITY_ENV=development

# 로깅
LOG_LEVEL=INFO
LOG_FILE=trinity_os.log

# 모니터링
HEALTH_CHECK_INTERVAL=60
PROBLEM_SCAN_INTERVAL=300

# 외부 서비스 (선택사항)
# REDIS_URL=redis://localhost:6379
# DATABASE_URL=postgresql://user:pass@localhost:5432/trinity
# API_BASE_URL=http://localhost:8000

# 보안 (절대 하드코딩하지 마세요)
# SECRET_KEY=your-secret-key-here
# API_KEY=your-api-key-here

# Cursor 설정 (선택사항)
# CURSOR_REVIEW_ENABLED=false
# CURSOR_CHAT_ENABLED=true