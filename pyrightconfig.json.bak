{
  "include": [
    "packages/afo-core"
  ],
  "exclude": [
    "**/.git/**",
    "**/.venv*/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/site-packages/**",
    "**/*.metallib",
    "**/*.dylib",
    "**/*.so",
    "**/*.a",
    "**/libtorch*/**",
    "packages/trinity-os/tests",
    "packages/afo-core/tests",
    "packages/afo-core/scripts",
    "packages/afo-core/legacy",
    "packages/afo-core/tigers",
    "packages/afo-core/verify_*.py",
    "packages/afo-core/strategists",
    "packages/afo-core/stubs.py",
    "packages/afo-core/*.py",
    "packages/afo-core/memory",
    "packages/afo-core/julie_cpa",
    "packages/afo-core/domain",
    "packages/afo-core/services",
    "packages/afo-core/api",
    "packages/afo-core/utils",
    "packages/afo-core/cache",
    "packages/afo-core/config",
    "packages/afo-core/scholars",
    "packages/afo-core/llms",
    "packages/afo-core/factories",
    "packages/afo-core/mediators",
    "packages/afo-core/browser_auth",
    "packages/afo-core/validation",
    "packages/afo-core/examples",
    "packages/afo-core/afo",
    "packages/afo-core/afo_soul_engine",
    "packages/afo-core/rag_*",
    "packages/afo-core/alignment",
    "packages/afo-core/kms",
    "tests"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",
  "executionEnvironments": [
    {
      "root": "packages/afo-core",
      "extraPaths": [
        "packages/afo-core",
        "packages/trinity-os"
      ],
      "diagnosticSeverityOverrides": {
        "reportMissingImports": "warning"
      },
      "typeCheckingMode": "strict"
    },
    {
      "root": "packages/trinity-os",
      "extraPaths": [
        "packages/afo-core",
        "packages/trinity-os"
      ],
      "diagnosticSeverityOverrides": {
        "reportMissingImports": "none"
      },
      "typeCheckingMode": "basic"
    },
    {
      "root": "scripts",
      "typeCheckingMode": "basic"
    },
    {
      "root": "legacy",
      "typeCheckingMode": "off"
    }
  ]
}
